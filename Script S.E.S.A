javascript:(function(){if(document.getElementById("sesaProConsole")||document.getElementById("sesaKeyCheck"))return;const chaveCorreta="sesaXIT";const chaveSalva=localStorage.getItem("sesaKey");const expiraEm=86400000;const agora=Date.now();if(chaveSalva){try{const dados=JSON.parse(chaveSalva);if(dados.chave===chaveCorreta&&(agora-dados.tempo)<expiraEm){iniciarSesaPro();return;}}catch(e){}}const keyDiv=document.createElement("div");keyDiv.id="sesaKeyCheck";Object.assign(keyDiv.style,{position:"fixed",top:"0",left:"0",width:"100vw",height:"100vh",background:"#000d",display:"flex",alignItems:"center",justifyContent:"center",zIndex:"999999",fontFamily:"monospace",color:"#0f0",flexDirection:"column"});keyDiv.innerHTML=`<div style="margin-bottom:10px;font-size:18px;">üîê S.E.S.A PRO // Insira sua chave de acesso</div><input id="sesaKeyInput" type="text" placeholder="Digite a chave..." style="padding:8px;background:#111;border:1px solid #0f0;color:#0f0;width:250px;font-size:14px;text-align:center;border-radius:4px;"><button id="sesaKeyButton" style="margin-top:10px;padding:6px 12px;background:#0f0;color:#000;font-weight:bold;border:none;cursor:pointer;border-radius:4px;">DESTRAVAR</button><div id="sesaKeyMsg" style="margin-top:10px;color:red;font-size:14px;"></div>`;document.body.appendChild(keyDiv);document.getElementById("sesaKeyButton").onclick=()=>{const key=document.getElementById("sesaKeyInput").value.trim();if(key===chaveCorreta){localStorage.setItem("sesaKey",JSON.stringify({chave:key,tempo:Date.now()}));keyDiv.remove();iniciarSesaPro();}else{document.getElementById("sesaKeyMsg").textContent="‚ùå Chave inv√°lida. Tente novamente.";}};function iniciarSesaPro(){if(document.getElementById("sesaProConsole"))return;const loading=document.createElement("div");loading.id="sesaLoading";Object.assign(loading.style,{position:"fixed",top:"0",left:"0",width:"100vw",height:"100vh",background:"#000d",display:"flex",alignItems:"center",justifyContent:"center",zIndex:"999998",color:"#0f0",fontFamily:"monospace",fontSize:"16px",userSelect:"none",cursor:"default"});loading.textContent="S.E.S.A PRO // Carregando n√∫cleo...";document.body.appendChild(loading);setTimeout(()=>{loading.textContent="Clique onde deseja fixar o console...";loading.style.cursor="crosshair";function onClick(e){const clickX=e.clientX;const clickY=e.clientY;loading.remove();document.removeEventListener("click",onClick);const consoleWidth=400;const consoleHeight=250;const posX=(clickX+consoleWidth>window.innerWidth)?window.innerWidth-consoleWidth-10:clickX;const posY=(clickY+consoleHeight>window.innerHeight)?window.innerHeight-consoleHeight-10:clickY;iniciarConsole(posX,posY);}document.addEventListener("click",onClick);},1200);function iniciarConsole(posX,posY){const root=document.createElement("div");root.id="sesaProConsole";Object.assign(root.style,{position:"fixed",left:posX+"px",top:posY+"px",background:"#1a1a1a",color:"#ccc",fontFamily:"monospace",border:"1px solid #333",borderRadius:"4px",padding:"12px",width:"400px",zIndex:"999999",boxShadow:"0 0 10px rgba(0,0,0,0.5)",userSelect:"none"});root.innerHTML=`<div id="sesaProClose" style="position:absolute;top:6px;right:10px;color:red;font-weight:bold;cursor:pointer;user-select:none;">[x]</div><div id="sesaProTitle" style="margin:0 0 10px;font-size:13px;color:#0f0;border-bottom:1px solid #333;padding-bottom:4px;user-select:none;">S.E.S.A // N√∫cleo de Execu√ß√£o Profissional</div><select id="sesaProSelect" style="width:100%;margin-top:8px;background:#111;color:#0f0;border:1px solid #333;font-family:monospace;font-size:13px;padding:6px;user-select:none;"><option value="">-- Selecione uma A√ß√£o Profissional --</option><option value="0">Destruir CSS da p√°gina</option><option value="1">Injetar c√≥digo remoto</option><option value="2">Interceptar XHR</option><option value="3">Remover eventos DOM</option><option value="4">Silenciar console</option><option value="5">For√ßar logout</option><option value="6">Corromper DOM</option><option value="7">Bloquear intera√ß√£o</option><option value="8">Loop reload</option><option value="9">Espelhar interface</option><option value="10">Listar links externos</option><option value="11">Keylogger b√°sico</option><option value="12">Observar DOM</option><option value="13">Scroll autom√°tico</option><option value="14">Exibir texto da p√°gina</option><option value="15">Remover formul√°rios</option><option value="16">P√°gina edit√°vel</option><option value="17">Monitorar cliques</option><option value="18">Interceptar navega√ß√£o</option><option value="19">Cancelar timeouts</option><option value="20">Executar JavaScript Personalizado</option><option value="21">Exibir Hitbox Neon</option></select><button id="sesaProRun" style="width:100%;margin-top:8px;background:#111;color:#0f0;border:1px solid #333;font-family:monospace;font-size:13px;padding:6px;user-select:none;cursor:pointer;">EXECUTAR COMANDO</button>`;document.body.appendChild(root);document.getElementById("sesaProClose").onclick=()=>root.remove();document.getElementById("sesaProRun").onclick=()=>{const idx=document.getElementById("sesaProSelect").value;if(idx==="")return;try{switch(parseInt(idx)){case 0:document.querySelectorAll("style,link[rel=stylesheet]").forEach(e=>e.remove());break;case 1:fetch("https://raw.githubusercontent.com/...").then(r=>r.text()).then(eval);break;case 2:XMLHttpRequest.prototype.realOpen=XMLHttpRequest.prototype.open;XMLHttpRequest.prototype.open=function(){console.warn("[SNIFFED]",arguments);return XMLHttpRequest.prototype.realOpen.apply(this,arguments);};break;case 3:document.querySelectorAll("*").forEach(e=>e.outerHTML=e.outerHTML);break;case 4:console.log=()=>{};console.warn=()=>{};console.error=()=>{};break;case 5:localStorage.clear();sessionStorage.clear();location.reload();break;case 6:document.body.innerHTML="<h1 style='color:#0f0'>Sistema Interrompido</h1>";break;case 7:document.body.style.pointerEvents="none";break;case 8:setInterval(()=>location.reload(),1000);break;case 9:document.body.style.transform="scaleX(-1)";break;case 10:console.log([...document.links].filter(a=>a.href.startsWith("http")).map(a=>a.href).join("\n"));break;case 11:document.addEventListener("keydown",e=>console.log("[KEY]",e.key));break;case 12:new MutationObserver(m=>console.log("[MUT]",m)).observe(document.body,{childList:true,subtree:true});break;case 13:setInterval(()=>window.scrollBy(0,50),100);break;case 14:console.log(document.body.innerText);break;case 15:document.querySelectorAll("form").forEach(f=>f.remove());break;case 16:document.body.contentEditable=true;break;case 17:document.addEventListener("click",e=>console.log("Click:",e.clientX,e.clientY));break;case 18:history.pushState=(f=>function(...a){console.log("[NAV]",a);return f.apply(this,a);})(history.pushState);window.addEventListener("hashchange",()=>console.log("[HASH]",location.hash));break;case 19:let i=0;while(i<9999){clearInterval(i);clearTimeout(i);i++;}break;case 20:const customBox=document.createElement("div");Object.assign(customBox.style,{position:"fixed",top:"50%",left:"50%",transform:"translate(-50%,-50%)",width:"90%",maxWidth:"500px",background:"#111",color:"#0f0",zIndex:"999999",padding:"10px",border:"1px solid #0f0",borderRadius:"8px",boxShadow:"0 0 10px #0f0"});customBox.innerHTML=`<div style="text-align:right"><button id="customClose" style="color:red;background:none;border:none;font-size:16px;cursor:pointer;">[x]</button></div><textarea id="customCode" style="width:100%;height:150px;background:#000;color:#0f0;font-family:monospace;padding:8px;border:1px solid #333;border-radius:4px;" placeholder="Cole seu c√≥digo JavaScript aqui..."></textarea><button id="customRun" style="margin-top:8px;width:100%;background:#0f0;color:#000;padding:6px;font-weight:bold;cursor:pointer;">Executar</button>`;document.body.appendChild(customBox);document.getElementById("customClose").onclick=()=>customBox.remove();document.getElementById("customRun").onclick=()=>{try{eval(document.getElementById("customCode").value);}catch(err){alert("Erro ao executar: "+err);}};break;case 21:document.querySelectorAll("*").forEach(e=>{e.style.outline="2px solid red";e.style.boxShadow="0 0 5px red, 0 0 15px red";});break;default:break;}console.log(`[S.E.S.A] Comando "${document.getElementById("sesaProSelect").selectedOptions[0].text}" executado.`);}catch(e){console.error("[S.E.S.A] Erro na execu√ß√£o:",e);}};}}})();
